<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_mentalk"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_mentalk1"
        android:supportsRtl="true"
        android:theme="@style/Theme.MentalkApp"
        tools:targetApi="31">
        <activity
            android:name=".view.test.TestActivity"
            android:exported="false" />
        <activity
            android:name=".view.result.ResultActivity"
            android:exported="false" />
        <activity
            android:name=".view.overview.OverviewActivity"
            android:exported="false" />
        <activity
            android:name=".view.splash.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".view.main.MainActivity"
            android:exported="true"></activity>
    </application>

</manifest>